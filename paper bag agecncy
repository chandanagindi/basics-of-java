import java.util.*;
public class Paper
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        int d=a*b;
        int e=a*c;
        int f=d-e;
        int g=f-100;
        System.out.println(g);
    }
}
